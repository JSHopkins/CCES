  <script>
      var myCalendar = createCalendar({
        options: {
        },
        data: {
          title: 'Holy Thursday Mass',     // Event title
          start: new Date('April 09, 2020 19:00 EDT'),   // Event start date
          duration: 60,                            // Event duration (IN MINUTES)
          end: new Date('April 09, 2020 20:00 EDT'),     // You can also choose to set an end time.
